import java.io.File;
import java.io.IOException;

public class Test {

	public static void main(String[] args) {
		String ans = null;
		try {
			ans = FileUtils.readFrom(new File("haha.txt"));
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

		System.out.println(ans);
	}

}
