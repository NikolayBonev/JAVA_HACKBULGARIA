
public class Task33_100SMS {
	
	static String numbersToMessage(int[] pressedSequence){
		
		StringBuilder message = new StringBuilder();
		
		for(int i = 0 ; i< pressedSequence.length ; i++){
			
			switch (pressedSequence[i]) {
			case 0:
				message.append(" ");
				break;

			default:
				break;
			}
		}
	}
}
