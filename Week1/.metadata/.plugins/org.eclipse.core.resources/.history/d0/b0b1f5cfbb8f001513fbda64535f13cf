import java.util.GregorianCalendar;

public class Task34_Friday_Years {
	static int friday_years(String start, String end){
		
		Integer startYear = Integer.parseInt(start);
		Integer endYear = Integer.parseInt(end);
		
		int years = -1;
		GregorianCalendar calen = new GregorianCalendar();
		GregorianCalendar firstDay = new GregorianCalendar();
		
		for(int i = startYear ; i <= endYear ; i++){
			calen.set(i, 12, 31);
			firstDay.set(i, 1, 1);
			
			if(calen.isLeapYear(i) && (firstDay.getFirstDayOfWeek() == 5 || firstDay.getFirstDayOfWeek() == 6)){
				years++;
			}else if(!calen.isLeapYear(i) && firstDay.getFirstDayOfWeek() == 5){
				years++;
			}
		}
		
		return years;
	}
}
