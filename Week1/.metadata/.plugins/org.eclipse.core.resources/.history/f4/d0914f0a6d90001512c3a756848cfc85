
public class Test {

	public static void main(String[] args) {
		
		int[] message = {1, 4, 4, 4, 8, 8, 8, 6, 6, 6, 0, 3, 3, 0, 1, 7, 7, 7, 7, 7, 2, 6, 6, 3, 2};
		
		message = Task33_100SMS.messageToNumbers("aabbcc");
		
		for(int i : message){
			System.out.print(i + " ");
		}
		System.out.println();
	}

}
